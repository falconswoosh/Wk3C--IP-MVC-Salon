@{
  Layout = "_Layout";
}

<h1 id="stylists">Stylists</h1>
  <ul>

    @if (Model != null)
    {
    @foreach (var stylist in Model)
    {
    <li class="stylistsOut"><a href="/stylist/@stylist.Id">@stylist.LastName, @stylist.FirstName</a></li>
    }
    }
    else if (Model == null)
    {
    <p> Stylist List is blank - create a new stylist now.</p>
    }
  </ul>
